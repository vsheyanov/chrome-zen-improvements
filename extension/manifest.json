{
    "manifest_version": 3,
    "name": "Tab Manager & Logger",
    "version": "1.3.0",
    "description": "Chrome extension that detects New Tab button clicks and moves those tabs to the beginning",
    "permissions": [
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Tab Manager & Logger",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}